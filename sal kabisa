def is_leap_year(year):
        if (year % 4 == 0 and year % 100 != 0) or (year % 400 == 0):
                        return True
        return False
year = int(input("yak sal ra vard konid: "))
if is_leap_year(year):
            print(f"sal{year} kabisa ast.")
else:
            print(f"sal {year} kabisa nist.")
        
